2/website/client/src/components/LoginForm.jsx",["163"],[],"import React, { useState, useEffect } from 'react';\nimport { Link, useNavigate } from 'react-router-dom'\nimport { useAuth } from '../AuthContext';\nimport axios from 'axios';\n\nfunction LoginForm() {\n    const navigate = useNavigate();\n\n    const [EmailId, setEmail] = useState('');\n    const [Password, setPassword] = useState('');\n    const [rememberMe, setRememberMe] = useState(false);\n    const [errorMessage, setErrorMessage] = useState('');\n    const [animatedText, setAnimatedText] = useState('');\n    const [isLoading, setIsLoading] = useState(false);\n\n    const {setLoggedIn} = useAuth();\n\n    useEffect(() => {\n        // Animate the \"MaidEase\" text\n        const animateText = async () => {\n            const text = 'MaidEase';\n            for (let i = 0; i <= text.length; i++) {\n                await new Promise((resolve) => {\n                    setTimeout(() => {\n                        setAnimatedText(text.slice(0, i));\n                        resolve();\n                    }, 50 * i);\n                });\n            }\n        };\n\n        animateText();\n    }, []);\n\n    const handleSubmit = async (e) => {\n        e.preventDefault();\n      \n        // Make an API call to your server for authentication\n        try {\n            const response = await axios.post('http://localhost:4000/login', {\n              EmailId,\n              Password,\n            });\n        \n            if (response.status === 200) {\n              localStorage.setItem('authToken', response.data.token);\n              setLoggedIn(true);\n              navigate('/');\n            } else {\n              setErrorMessage('Incorrect email or password');\n            }\n          } catch (error) {\n            console.error('Error logging in:', error);\n            setErrorMessage('Error logging in. Please try again later.');\n          }\n      };\n      \n\n    const handleInputChange = (setter) => (e) => {\n        setter(e.target.value);\n        setErrorMessage('');\n    };\n\n    const clearInput = (setter) => () => {\n        setter('');\n    };\n\n    return (\n        <div className=\"relative flex flex-1 flex-col items-center justify-center p-8\">\n            <div className=\"absolute top-0 mt-4 ml-4 text-xl font-bold\">\n                <span className='text-purple-400'>Maid</span><span className='text-[#50C878]'>Ease</span>\n            </div>\n            <div className=\"w-full max-w-xs text-left\">\n                <h1 className=\"mb-2 text-2xl font-bold\">\n                    Welcome to <span className='text-gray-700'>{animatedText}</span>\n                </h1>\n                <p className=\"mb-6 text-base text-gray-600\">\n                    Your all-in-one home services solution\n                </p>\n            </div>\n            <form\n                onSubmit={handleSubmit}\n                className=\"w-full max-w-xs text-left space-y-4\"\n            >\n                <div className=\"relative\">\n                    <input\n                        id=\"email\"\n                        type=\"email\"\n                        name=\"email\"\n                        placeholder=\"Email\"\n                        value={EmailId}\n                        onChange={handleInputChange(setEmail)}\n                        className={`w-full px-3 py-2 border ${errorMessage ? 'border-red-500' : 'border-gray-300'\n                            } rounded-md`}\n                        required\n                    />\n                    {errorMessage && EmailId !== 'test@example.com' && (\n                        <span\n                            onClick={clearInput(setEmail)}\n                            className=\"absolute right-2 top-1/2 transform -translate-y-1/2 cursor-pointer\"\n                        >\n                            &#x2715;\n                        </span>\n                    )}\n                </div>\n                <div className=\"relative\">\n                    <input\n                        id=\"password\"\n                        type=\"password\"\n                        name=\"password\"\n                        placeholder=\"Password\"\n                        value={Password}\n                        onChange={handleInputChange(setPassword)}\n                        className={`w-full px-3 py-2 border ${errorMessage ? 'border-red-500' : 'border-gray-300'\n                            } rounded-md`}\n                        required\n                    />\n                    {errorMessage && Password !== 'password' && (\n                        <span\n                            onClick={clearInput(setPassword)}\n                            className=\"absolute right-2 top-1/2 transform -translate-y-1/2 cursor-pointer\"\n                        >\n                            &#x2715;\n                        </span>\n                    )}\n                </div>\n                {errorMessage && (\n                    <div className=\"text-red-500 text-sm\">{errorMessage}</div>\n                )}\n                <div className=\"flex items-center justify-between\">\n                    <label htmlFor=\"remember\" className=\"text-sm\">\n                        <input\n                            id=\"remember\"\n                            type=\"checkbox\"\n                            checked={rememberMe}\n                            onChange={(e) => setRememberMe(e.target.checked)}\n                            className=\"mr-1\"\n                        />\n                        Remember me\n                    </label>\n                    <button\n                        type=\"button\"\n                        // onClick={() => alert('Forgot password functionality not implemented yet.')}\n                        className=\"text-sm text-purple-500 hover:text-purple-700\"\n                    >\n                        Forgot password?\n                    </button>\n                </div>\n                <button\n      type=\"submit\"\n      className=\"w-full py-2 mt-4 bg-purple-700 text-white font-semibold rounded-md hover:bg-purple-800 hover:text-xl transition-all duration-200\"\n      disabled={isLoading}\n    >\n      {isLoading ? (\n        <div className=\"w-5 h-5 border-t-2 border-white rounded-full animate-spin\"></div>\n      ) : (\n        'Sign in'\n      )}\n    </button>\n            </form>\n            <div className=\"mt-4 text-sm\">\n                Don't have an account?{' '}\n                <Link to='/register'>\n                    <button\n                        //   onClick={() => alert('Sign up functionality not implemented yet.')}\n                        className=\"text-purple-500 hover:text-purple-700\"\n                    >\n                        Sign up\n                    </button>\n                </Link>\n            </div>\n        </div>\n    );\n}\n\nexport default LoginForm;\n","/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/Onboarding.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/Footer.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/ContactUs.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/Navbar.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/DeleteAccount.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/FavouriteCards.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/EditProfileModal.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/ServiceCard.jsx",["164"],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/DevelopmentInProgress.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/LoaderSpinner.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/TrackingLoyalty.jsx",["165"],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/ServiceCardDetails.jsx",["166","167","168","169"],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/Pages/OrderHistory.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/Pages/StatisticalData.jsx",[],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/Pages/AddToCart.jsx",["170"],[],"/Users/rushdavahora/Desktop/Developer/Maid-Ease/CS692/website/client/src/components/AddToCartCards.jsx",["171","172"],[],"import React, { useEffect, useState } from 'react';\nimport axios from 'axios';\nimport '../css/ServiceCard.css';\nimport 'font-awesome/css/font-awesome.min.css';\nimport { useNavigate } from 'react-router-dom';\nimport LoaderSpinner from '../components/LoaderSpinner';\n\nconst AddToCartCards = ({data, onRemove}) => {\n    const navigate = useNavigate();\n    const [loading, setLoading] = useState(true);\n\n    const handleRemove = () => {\n        axios\n          .delete(`http://localhost:4000/cartdetails/${data.CartDetailId}`)\n          .then((response) => {\n            if (response.status === 204) {\n              // Removal was successful\n              alert('Removed from your Cart.');\n              onRemove(data.CartDetailsId); // Ensure you're removing the correct item ID\n              navigate('/add-to-cart/1'); // Navigate back to cart or to the relevant page\n            } else {\n              console.error('Unexpected response status:', response.status);\n            }\n          })\n          .catch((error) => {\n            console.error('Error removing item from cart:', error);\n          })\n          .finally(() => {\n            // Reset the loading state once the request is completed\n            setLoading(false);\n          });\n    };\n    \n    const handleBookService = () => {\n      const payload = {\n          UserId: data.UserId,\n          ServiceProviderId1: data.ServiceProviderId,\n          ServiceProviderId2 : null,\n          ServiceProviderId3: null,\n          NetAmount: data.DiscountedPrice,\n          Tax: ((data.DiscountedPrice/100) * 3.5).toFixed(2),\n          Discount: 10,\n          TotalPrice:((data.DiscountedPrice + ((data.DiscountedPrice/100) * 3.5)) - 10).toFixed(2),\n          BookingStartDate: new Date(),\n          BookingEndDate: new Date(),\n          IsActive: true,\n          CreatedBy: \"System\",\n          UpdatedBy : \"\"\n      };\n\n      axios\n          .post('http://localhost:4000/bookingdetails', payload)\n          .then((response) => {\n              if (response.status === 201) {\n                  handleRemove();\n                  alert('Service booked successfully.');\n                  // Additional logic after booking (like navigating to another page if needed)\n              } else {\n                  console.error('Unexpected response status:', response.status);\n              }\n          })\n          .catch((error) => {\n              console.error('Error booking service:', error);\n          });\n  };\n\n    useEffect(() => {\n      const loadData = async () => { \n        // Wait for two second\n        await new Promise((r) => setTimeout(r, 1000));\n        // Toggle loading state\n        setLoading((loading) => !loading);\n    };\n    loadData();\n  }, []);\n\n    return (\n        <div className=\"product-card\">\n        { data.IsHotDeal ? <div className=\"badge\">Hot</div> : null }\n        <div className=\"product-tumb\">\n          <img src={data.ImageURL} alt=\"\"/>\n        </div>\n        <div className=\"product-details\">\n          <span className=\"product-catagory\">{data.ServiceType}</span>\n          <h4>{data.FirstName}  {data.LastName}</h4>\n          <span class=\"ratings-yellow-star\"><b>{data.Ratings}</b> <i class=\"fa fa-star ratings-yellow-star\" aria-hidden=\"true\"></i></span>\n          <p>{data.Description}</p>\n          <p><b>Address: {data.Address1}, {data.City}, {data.State} {data.ZipCode}<br/>\n          ContactNo: {data.ContactNo}</b></p>\n          <div className=\"product-bottom-details\">\n            <div className=\"product-price\"><span  style={{textDecoration: 'line-through'}}>${data.OriginalPrice}</span> &nbsp;${data.DiscountedPrice}</div>\n            <div className=\"product-links\">\n            <span><button className='favourite-delete' onClick={handleRemove}>Remove</button></span> &emsp; \n              <span><button className='favourite-delete' onClick={handleBookService}>Book Service</button></span>\n            </div>\n          </div>\n        </div>\n      </div>\n)};\n\nexport default AddToCartCards;",{"ruleId":"173","severity":1,"message":"174","line":9,"column":8,"nodeType":"175","messageId":"176","endLine":9,"endColumn":19},{"ruleId":"173","severity":1,"message":"177","line":10,"column":8,"nodeType":"175","messageId":"176","endLine":10,"endColumn":16},{"ruleId":"173","severity":1,"message":"178","line":11,"column":8,"nodeType":"175","messageId":"176","endLine":11,"endColumn":16},{"ruleId":"179","severity":1,"message":"180","line":115,"column":10,"nodeType":"181","endLine":115,"endColumn":21,"suggestions":"182"},{"ruleId":"173","severity":1,"message":"183","line":8,"column":8,"nodeType":"175","messageId":"176","endLine":8,"endColumn":14},{"ruleId":"179","severity":1,"message":"184","line":17,"column":8,"nodeType":"181","endLine":17,"endColumn":10,"suggestions":"185"},{"ruleId":"173","severity":1,"message":"186","line":12,"column":10,"nodeType":"175","messageId":"176","endLine":12,"endColumn":28},{"ruleId":"173","severity":1,"message":"186","line":12,"column":10,"nodeType":"175","messageId":"176","endLine":12,"endColumn":28},{"ruleId":"173","severity":1,"message":"187","line":14,"colum